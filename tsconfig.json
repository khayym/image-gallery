{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "components/*": ["src/components/*"],
      "icons/*": ["./assets/icons/*"],
      "images/*": ["./assets/images/*"],
      "configs/*": ["./src/configs/*"],
      "helpers/*": ["./src/helpers/*"],
      "screens/*": ["./src/screens/*"],
      "store/*": ["./src/store/*"],
      "theme/*": ["./src/theme/*"],
      "constants/*": ["./src/constants/*"],
      "types/*": ["./src/types/*"]
    },
    "moduleSuffixes": [".ios", ".android", ".native", ""],
    "lib": ["ES2021"]
  }
}
